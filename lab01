#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5aaec9a12880 .scope module, "lab01_tb" "lab01_tb" 2 18;
 .timescale -9 -12;
v0x5aaec9a2fd70_0 .var "clk", 0 0;
v0x5aaec9a2fe10_0 .var/i "count", 31 0;
v0x5aaec9a2fed0_0 .var/i "failedTests", 31 0;
v0x5aaec9a2ff90_0 .var/i "high_count", 31 0;
v0x5aaec9a30070_0 .var "last_tick", 0 0;
v0x5aaec9a30180_0 .var "reset", 0 0;
v0x5aaec9a30240_0 .net "tick_100_2", 0 0, L_0x5aaec9a02180;  1 drivers
v0x5aaec9a302e0_0 .var/i "totalTests", 31 0;
v0x5aaec9a303a0_0 .var/i "transition_count", 31 0;
E_0x5aaec99d9960 .event negedge, v0x5aaec9a30180_0;
S_0x5aaec9a12a10 .scope module, "uut_100_2" "gen_tick" 2 43, 3 16 0, S_0x5aaec9a12880;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "src_clk";
    .port_info 1 /INPUT 1 "enable";
    .port_info 2 /OUTPUT 1 "tick";
P_0x5aaec9a00290 .param/l "SRC_FREQ" 0 3 16, +C4<00000000000000000000000001100100>;
P_0x5aaec9a002d0 .param/l "TICK_FREQ" 0 3 16, +C4<00000000000000000000000000000010>;
L_0x5aaec9a02180 .functor BUFZ 1, v0x5aaec9a2fd70_0, C4<0>, C4<0>, C4<0>;
L_0x72d4441b5018 .functor BUFT 1, C4<1>, C4<0>, C4<0>, C4<0>;
v0x5aaec9a022a0_0 .net "enable", 0 0, L_0x72d4441b5018;  1 drivers
v0x5aaec9a02e00_0 .net "src_clk", 0 0, v0x5aaec9a2fd70_0;  1 drivers
v0x5aaec9a2fc50_0 .net "tick", 0 0, L_0x5aaec9a02180;  alias, 1 drivers
E_0x5aaec99d9e40 .event posedge, v0x5aaec9a02e00_0;
    .scope S_0x5aaec9a12a10;
T_0 ;
    %wait E_0x5aaec99d9e40;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5aaec9a12880;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a302e0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a2fed0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a2fe10_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a2ff90_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aaec9a30070_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a303a0_0, 0, 32;
    %end;
    .thread T_1;
    .scope S_0x5aaec9a12880;
T_2 ;
    %vpi_call 2 31 "$dumpfile", "lab01.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5aaec9a12880;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aaec9a2fd70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5aaec9a30180_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5aaec9a2fd70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5aaec9a30180_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aaec9a2fd70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aaec9a30180_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5aaec9a2fd70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aaec9a30180_0, 0, 1;
    %delay 50000, 0;
T_3.0 ;
    %load/vec4 v0x5aaec9a2fd70_0;
    %inv;
    %store/vec4 v0x5aaec9a2fd70_0, 0, 1;
    %delay 50000, 0;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x5aaec9a12880;
T_4 ;
    %wait E_0x5aaec99d9960;
    %wait E_0x5aaec99d9e40;
    %delay 10000, 0;
    %vpi_call 2 83 "$write", "Test Source clock 100Hz, Tick 2Hz ... " {0 0 0};
    %pushi/vec4 1, 0, 32;
    %assign/vec4 v0x5aaec9a302e0_0, 0;
T_4.0 ;
    %load/vec4 v0x5aaec9a2fe10_0;
    %cmpi/s 1000, 0, 32;
    %jmp/0xz T_4.1, 5;
    %wait E_0x5aaec99d9e40;
    %load/vec4 v0x5aaec9a30070_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5aaec9a30240_0;
    %load/vec4 v0x5aaec9a30070_0;
    %cmp/ne;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.2, 8;
    %load/vec4 v0x5aaec9a303a0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5aaec9a303a0_0, 0;
T_4.2 ;
    %load/vec4 v0x5aaec9a2fe10_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5aaec9a2fe10_0, 0, 32;
    %load/vec4 v0x5aaec9a30240_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_4.4, 4;
    %load/vec4 v0x5aaec9a2ff90_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5aaec9a2ff90_0, 0;
T_4.4 ;
    %load/vec4 v0x5aaec9a30240_0;
    %assign/vec4 v0x5aaec9a30070_0, 0;
    %jmp T_4.0;
T_4.1 ;
    %load/vec4 v0x5aaec9a2ff90_0;
    %pushi/vec4 500, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5aaec9a303a0_0;
    %pushi/vec4 20, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.6, 8;
    %vpi_call 2 98 "$display", "PASSED" {0 0 0};
    %jmp T_4.7;
T_4.6 ;
    %vpi_call 2 100 "$display", "FAILED" {0 0 0};
    %load/vec4 v0x5aaec9a2fed0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5aaec9a2fed0_0, 0, 32;
T_4.7 ;
    %pushi/real 1073741824, 4066; load=1.00000
    %load/vec4 v0x5aaec9a2ff90_0;
    %cvt/rv/s;
    %mul/wr;
    %load/vec4 v0x5aaec9a2fe10_0;
    %cvt/rv/s;
    %div/wr;
    %vpi_call 2 103 "$display", "Load (%d/%d): %0.2f", v0x5aaec9a2ff90_0, v0x5aaec9a2fe10_0, W<0,r> {0 1 0};
    %vpi_call 2 104 "$display", "Transition count: %d", v0x5aaec9a303a0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5aaec9a30070_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a303a0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a2fe10_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aaec9a2ff90_0, 0, 32;
    %vpi_call 2 117 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
